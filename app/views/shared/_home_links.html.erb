<h1 class="light">Quick Links:</h1>
<h2>Visiting Iona</h2>
<ul>
	<% Page.find_by_name("visiting-iona").subpages.each do |sp| %>
	<li><%= link_to sp.navlabel, viewer_sub_path("visiting-iona",sp.name.gsub("'","$").gsub(" ","_")) %></li>
	<% end %>
<!---->
</ul>
	
<h2>Iona Maps</h2>
<ul>
	<% Map.all.delete_if{|ca| ca.category.present?}.each do |m| %>
		<li><%= link_to m.title,map_path(m.id) %></li>
	<% end %>
	<% Category.find_all_sub.delete_if{|ca| ca.maps.count == 0}.each do |c| %>
		<li><%= link_to c.title,mapper_show_subfiltered_path(c.parent.page.name,c.parent.title.gsub("/","+").gsub(" ","_"),c.title.gsub("/","+")) %></li>
	<% end %>

</ul>